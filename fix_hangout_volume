plutil -convert xml1  ~/Library/Preferences/com.google.GoogleTalkPluginD.plist
cat ~/Library/Preferences/com.google.GoogleTalkPluginD.plist | awk '// {if (flags == 1) {printf "        <string>1</string>\n"; flags=0} else {printf "%s\n", $0}} />audio-flags/ {flags=1;} ' > .temp_plist
cat .temp_plist
mv .temp_plist ~/Library/Preferences/com.google.GoogleTalkPluginD.plist
plutil -convert binary1 ~/Library/Preferences/com.google.GoogleTalkPluginD.plist

